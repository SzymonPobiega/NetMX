<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="netMXConnectorFactory" type="Simon.Configuration.Provider.ProviderConfigurationSection, Simon.Configuration" />
    <section name="netMXProxyFactory" type="Simon.Configuration.Provider.ProviderConfigurationSectionWithDefault, Simon.Configuration" />
  </configSections>
  <system.web>
    <authentication mode="Windows" />
    <identity impersonate="true" />
    <compilation debug="true">
      <assemblies>
      </assemblies>
      <buildProviders>
        <add extension="*.asbx" type="Microsoft.Web.Preview.Services.BridgeBuildProvider" />
      </buildProviders>
    </compilation>
  </system.web>
  <netMXConnectorFactory>
    <providers>
      <add name="tcp" type="NetMX.Remote.Remoting.RemotingConnectorProvider, NetMX.Remote.Remoting" />
    </providers>
  </netMXConnectorFactory>
  <netMXProxyFactory defaultProvider="castle">
    <providers>
      <add name="castle" type="NetMX.Proxy.Castle.CastleProxyProvider, NetMX.Proxy.Castle" />
    </providers>
  </netMXProxyFactory>
</configuration>