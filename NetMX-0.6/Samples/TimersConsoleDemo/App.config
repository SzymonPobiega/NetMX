<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<configSections>
		<section name="netMX" type="Simon.Configuration.Provider.ProviderConfigurationSectionWithDefault, Simon.Configuration"/>
    <section name="netMXProxyFactory" type="Simon.Configuration.Provider.ProviderConfigurationSectionWithDefault, Simon.Configuration"/>
	</configSections>
	<netMX defaultProvider="default">
		<providers>
			<add name="default" type="NetMX.Default.MBeanServerBuilder, NetMX.Default"/>
		</providers>
	</netMX>
  <netMXProxyFactory defaultProvider="castle">
    <providers>
      <add name="castle" type="NetMX.Proxy.Castle.CastleProxyProvider, NetMX.Proxy.Castle"/>
    </providers>
  </netMXProxyFactory>
</configuration>